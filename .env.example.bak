# ===========================================
# Supabase 설정 (필수)
# https://supabase.com 에서 프로젝트 생성
# ===========================================

# 데이터베이스 연결 (Settings > Database)
DATABASE_URL="postgresql://postgres:[PASSWORD]@db.[PROJECT].supabase.co:5432/postgres"
DIRECT_URL="postgresql://postgres:[PASSWORD]@db.[PROJECT].supabase.co:5432/postgres"

# Supabase API (Settings > API)
NEXT_PUBLIC_SUPABASE_URL="https://[PROJECT].supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="eyJ..."

# ===========================================
# 사이트 설정
# ===========================================
NEXT_PUBLIC_SITE_URL="http://localhost:3000"

# ===========================================
# 브랜딩 및 SEO 설정 (배포 시 필수 변경)
# ===========================================
NEXT_PUBLIC_APP_NAME="My SaaS"
NEXT_PUBLIC_APP_DESCRIPTION="나만의 SaaS 서비스"
NEXT_PUBLIC_COMPANY_NAME="회사명"
NEXT_PUBLIC_APP_ICON="🚀"
# SEO 키워드 (쉼표로 구분)
NEXT_PUBLIC_SEO_KEYWORDS="SaaS,스타트업,웹서비스,AI"

# ===========================================
# 사업자 정보 (Footer 표시용, 선택)
